<?xml version="1.0" encoding="utf-8" ?>
<doc>
	<marbleDiagrams>

		<!-- Time -->

		<marbleDiagram type="bullet" operator="AsInterval">
			<key>
				<symbol>-</symbol>
				<value>
					<paramref name="minimumPeriod"/>
				</value>
			</key>
			<input>
				<marbles>123--456|</marbles>
			</input>
			<output>
				<marbles>-1-2-3--4-5-6|</marbles>
			</output>
		</marbleDiagram>

		<marbleDiagram type="bullet" operator="AsTimer">
			<key>
				<symbol>-</symbol>
				<value>
					<paramref name="minimumPeriod"/>
				</value>
			</key>
			<input>
				<marbles>123--456|</marbles>
			</input>
			<output>
				<marbles>1-2-3--4-5-6|</marbles>
			</output>
		</marbleDiagram>

		<marbleDiagram type="bullet" operator="Pulse">
			<key>
				<symbol>-</symbol>
				<value>
					<paramref name="period"/>
				</value>
			</key>
			<input>
				<marbles>123---456|</marbles>
			</input>
			<output>
				<marbles>1-2-3-3-3-4-5-6|</marbles>
			</output>
		</marbleDiagram>

	</marbleDiagrams>
</doc>